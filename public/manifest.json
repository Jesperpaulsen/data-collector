{
  "name": "Waste Collector",
  "description": "Collecting your data usage",
  "version": "1.0.6",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Analyze your data usage"
  },
  "icons": {
    "16": "android-chrome-192x192.png",
    "48": "android-chrome-192x192.png",
    "128": "android-chrome-192x192.png"
  },
  "background": {
    "service_worker": "wrapper.js"
  },
  "content_scripts": [
    {
      "matches": [
            "<all_urls>"
        ],
        "run_at": "document_start",
        "js": [
            "content_script.js"
        ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}