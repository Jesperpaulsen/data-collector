  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{js,jsx,ts,tsx, json}": [
      "npx prettier --write"
    ]
  },
  "peerDependencies": {
    "postcss": "^8.1.10"
  },
  "resolutions": {
    "postcss": "8.1.10"
