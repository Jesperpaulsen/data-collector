{
  "name": "data-analyzer",
  "version": "1.0.0",
  "description": "Responsible for serving data-collector and data-presenter",
  "main": "src/index",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "dev:emulators": "export FIRESTORE_EMULATOR_HOST=localhost:8080 && export FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 && nodemon",
    "test": "jest --watchAll --no-cache --detectOpenHandles",
    "test:ci": "jest --verbose true --silent false",
    "emulators": "export GOOGLE_APPLICATION_CREDENTIALS=\"./appEngineKey.json\" && firebase emulators:start",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix",
    "build": "rm -rf ./dist && esbuild src/index.ts --define:process.env.NODE_ENV=\"production\" --bundle --sourcemap --outdir=./dist --platform=node",
    "update-ip": "node ./node_modules/geoip-country/scripts/updatedb.js",
    "gcp-build": "esbuild-node-tsc && npm run update-ip"
  },
  "author": "Jesper Paulsen",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.154",
    "@types/cors": "^2.8.12",
    "@types/geoip-country": "^4.0.0",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.7.12",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.12.29",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-tsc": "^1.6.1",
    "firebase": "^9.0.2",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/src/index.d.ts"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.1",
    "firebase-admin": "^9.11.1",
    "geoip-country": "^4.0.87"
  }
}