{
  "name": "data-analyzer",
  "version": "1.0.0",
  "description": "Responsible for serving data-collector and data-presenter",
  "main": "src/wrapper.ts",
  "scripts": {
    "dev": "esbuild src/** --define:process.env.NODE_ENV=\"production\" --bundle --sourcemap --outdir=./dist --watch",
    "test": "jest --watchAll --no-cache --detectOpenHandles",
    "test:ci": "jest",
    "emulators": "export GOOGLE_APPLICATION_CREDENTIALS=\"./appEngineKey.json\" && firebase emulators:start",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix",
    "build": "esbuild src/** --define:process.env.NODE_ENV=\"production\" --bundle --sourcemap --outdir=./dist --platform=node"
  },
  "author": "Jesper Paulsen",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.154",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.7.12",
    "@types/supertest": "^2.0.11",
    "esbuild-jest": "^0.5.0",
    "firebase": "^9.0.2",
    "node-fetch": "^3.0.0",
    "supertest": "^6.1.6"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/src/index.d.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.1",
    "firebase-admin": "^9.11.1"
  }
}