{
  "name": "Data Collector",
  "description": "Collecting your data usage",
  "version": "1.0.6",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Analyze your data usage"
  },
  "icons": {
    "16": "android-chrome-192x192.png",
    "48": "android-chrome-192x192.png",
    "128": "android-chrome-192x192.png"
  },
  "background": {
    "service_worker": "wrapper.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected-xhr.js",
        "injected-fetch.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "tabs",
    "alarms",
    "identity",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "154072078565-c2qtni1qj3atn2ip7cgdeofpph8jjhhk.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}